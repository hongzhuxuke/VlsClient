/*combobox统一格式*/
QComboBox#publinesCbx{border-bottom: 1px gray}
QComboBox#publinesCbx QAbstractItemView::item{height:30px;}
QComboBox
{
color: rgb(214, 214, 214);
font: 12px;
height: 28px;
min-height: 28px;
border-image:url(:/sysButton/comboBox_normal_big);

padding-left:10px;
padding-right:10px;
}

QComboBox::drop-down
{
right: 5px;
background:transparent;
subcontrol-origin: margin;
}

QComboBox::down-arrow
{
min-width: 25px;
min-height: 28px;
border:none;
image:url(:/sysButton/img/sysButton/down_arrow.png);
}

QComboBox::up-arrow
{
min-width: 25px;
min-height: 28px;
border:none;
image:url(:/sysButton/img/sysButton/arrorw_up.png);
}

QComboBox::down-arrow:on
{
min-width: 25px;
min-height: 28px;
border:none;
image:url(:/sysButton/img/sysButton/down_arrow.png);
}

QComboBox QAbstractItemView
{
	border:1px solid gray;
	height: 24px;
	outline: none;
	color: rgb(214, 214, 214);
	background-color: rgb(40, 40, 40);
	border-radius: 0px;
}

QComboBox QAbstractItemView::item {
	height: 24px;
	color: rgb(214, 214, 214);
	background-color: rgb(40, 40, 40);
	border-radius: 0px;
}

QComboBox QAbstractItemView::item:selected  
{     
	height: 24px;
	color: rgb(252, 86, 89);
	background-color: rgb(58, 58, 58);
	border-radius: 0px;
}

