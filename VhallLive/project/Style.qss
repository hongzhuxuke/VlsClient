QCheckBox {font: 12px bold; color: rgb(214, 214, 214);}
QCheckBox:disabled {font: 12px bold; color: rgb(142, 142, 142);}
QCheckBox::indicator {width: 15px; height: 15px;}
QCheckBox::indicator:checked {border-image:url(:/sysButton/img/sysButton/check.png);}
QCheckBox::indicator:unchecked {border-image:url(:/sysButton/img/sysButton/uncheck.png);}
QCheckBox::indicator:checked:disabled {border-image:url(:/sysButton/img/sysButton/check.png);}
QCheckBox::indicator:unchecked:disabled {border-image:url(:/sysButton/img/sysButton/uncheck.png);}

/*radiobutton统一格式*/
QRadioButton {font: 12px bold; color: rgb(214, 214, 214); }
