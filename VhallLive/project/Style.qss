/*音频配置弹出框*/
QSlider{
border-radius: 2px;
margin:0px 0px;
}
QSlider::groove:horizontal {
border: 1px solid #8d8c8c;
height: 2px;
margin: 0px 0px;
left: 5px;
right: 5px;
}
QSlider::groove:horizontal:disabled {
border: 1px solid #3B3A3A;
height: 2px;
margin: 0px 0px;
left: 5px;
right: 5px;
}
QSlider::handle:horizontal {
background: #D1D1D1;
border-radius: 5px;
border: 1px solid #8d8c8c;
width: 10px;
height: 12px;
margin: -5px -5px;
}
QSlider::handle:horizontal:disabled {
background: #6A6A6A;
border-radius: 5px;
border: 1px solid #6d6c6c;
width: 10px;
height: 12px;
margin: -5px -5px;
}
QSlider::sub-page:horizontal{
background: #FC5659;
}
QSlider::add-page:horizontal{
background: #5A5959;
}
QSlider::sub-page:horizontal:disabled{
background: #3B3A3A;
}
QSlider::add-page:horizontal:disabled{
background: #3B3A3A;
}

/*播放器控制条*/
QSlider#MediaPlayerSlider{
margin:0px 0px;
}
QSlider#MediaPlayerSlider::groove:horizontal {
height: 5px;
margin: 0px 0px;
left: 0px;
right: 0px;
}
QSlider#MediaPlayerSlider::handle:horizontal {
background: #f64840;
width: 1px;
height: 5px;
}
QSlider#MediaPlayerSlider::sub-page:horizontal{
background: #f64840;
}
QSlider#MediaPlayerSlider::add-page:horizontal{
background: #727272;
}


/*音频降噪条*/

QSlider#horizontalSlider_Noise{
border-radius: 0px;
margin:0px 0px;
}
QSlider#horizontalSlider_Noise::groove:horizontal {
border: 0px solid #8d8c8c;
height: 2px;
margin: 0px 0px;
left: 5px;
right: 5px;
}
QSlider#horizontalSlider_Noise::groove:horizontal:disabled {
border: 0px solid #3B3A3A;
height: 2px;
margin: 0px 0px;
left: 5px;
right: 5px;
}
QSlider#horizontalSlider_Noise::handle:horizontal {
background: #D1D1D1;
border-radius: 5px;
border: 1px solid #8d8c8c;
width: 10px;
height: 12px;
margin: -5px -5px;
}
QSlider#horizontalSlider_Noise::handle:horizontal:disabled {
background: #6A6A6A;
border-radius: 5px;
border: 1px solid #6d6c6c;
width: 10px;
height: 12px;
margin: -5px -5px;
}
QSlider#horizontalSlider_Noise::sub-page:horizontal{
	background-color:transparent
}
QSlider#horizontalSlider_Noise::add-page:horizontal{
	background-color:transparent
}
QSlider#horizontalSlider_Noise::sub-page:horizontal:disabled{
	background-color:transparent
}
QSlider#horizontalSlider_Noise::add-page:horizontal:disabled{
	background-color:transparent
}


/*播放器控制条*/
QSlider#MediaPlayerSlider1{
margin:0px 0px;
}
QSlider#MediaPlayerSlider1::groove:horizontal {
height: 5px;
